{
  "name": "infra-utils",
  "version": "1.0.0",
  "scripts": {
  "dev": "echo 'infra-utils has no dev server, only scripts'",

  "migrate": "ts-node scripts/migrate-db.ts",
  "seed": "ts-node scripts/seed-db.ts",
  "reset": "pnpm run migrate && pnpm run seed",

  "lint": "eslint 'src/**/*.ts' --max-warnings=0",
  "typecheck": "tsc --noEmit",

  "test": "vitest run",
  "test:watch": "vitest",

  "build": "tsc -p tsconfig.build.json",
  "clean": "rimraf dist"
}

}
